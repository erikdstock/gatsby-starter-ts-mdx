{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.30",
    "gatsby-mdx": "^0.4.2",
    "gatsby-plugin-catch-links": "^2.0.12",
    "gatsby-plugin-manifest": "^2.0.19",
    "gatsby-plugin-netlify-cms": "^3.0.12",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-plugin-sharp": "^2.0.22",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-plugin-typescript": "^2.0.9",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-images": "^3.0.6",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.1",
    "gatsby-transformer-sharp": "^2.1.14",
    "netlify-cms": "^2.6.0",
    "netlify-cms-widget-mdx": "^0.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "rebass": "^3.0.1",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6",
    "styled-system": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.121",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/rebass": "^3.0.3",
    "@types/styled-components": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-react": "^7.12.4",
    "graphql-typescript-definitions": "^0.15.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "netlify-cms-backend-fs": "^0.3.6",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333",
    "typescript-styled-plugin": "^0.13.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  }
}
